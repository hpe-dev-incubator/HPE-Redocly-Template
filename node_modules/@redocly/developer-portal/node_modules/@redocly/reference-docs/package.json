{
  "name": "@redocly/reference-docs",
  "version": "2.0.9",
  "description": "Redocly Reference Docs",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "author": "Roman Hotsiy <roman@redoc.ly>",
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": ">=12",
    "npm": ">=6.0.0"
  },
  "bin": {
    "redocly-api-reference": "bin/cli/commands/redocly-docs-cli.min.js",
    "reference-docs": "bin/cli/commands/redocly-docs-cli.min.js"
  },
  "dependencies": {
    "@redocly/openapi-core": "^1.0.0-beta.50",
    "@redocly/react-dropdown-aria": "2.0.12",
    "@redocly/vscode-json-languageservice": "3.4.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.59.4",
    "decko": "^1.2.0",
    "deepmerge": "^4.2.2",
    "dompurify": "^2.0.12",
    "eventemitter3": "^4.0.4",
    "fast-deep-equal": "^3.1.3",
    "handlebars": "^4.7.6",
    "hotkeys-js": "^3.7.6",
    "informed": "3.33.0",
    "js-yaml": "^3.13.0",
    "json-pointer": "^0.6.0",
    "kleur": "^3.0.3",
    "lunr": "^2.3.8",
    "mark.js": "^8.11.1",
    "marked": "^0.7.0",
    "mkdirp": "^0.5.1",
    "mobx-react": "^7.2.0",
    "node-libs-browser": "^2.2.0",
    "openapi-sampler": "^1.0.0",
    "perfect-scrollbar": "^1.4.0",
    "polished": "^3.6.5",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.6",
    "react-codemirror2": "^7.1.0",
    "react-tabs": "^3.1.1",
    "serve-static": "^1.14.1",
    "slugify": "^1.4.4",
    "stickyfill": "^1.1.1",
    "stringify-object": "^3.3.0",
    "styled-components": "^4.1.1 || ^5.1.1",
    "swagger-client": "^3.13.3",
    "swagger2openapi": "^6.2.1",
    "tslib": "^2.2.0",
    "url-template": "^2.0.8",
    "yargs": "^15.0.2"
  },
  "scripts": {
    "prepublishOnly-local": "npm run clean && npm run bundle && npm run compile-lib && npm run bundle-cli && npm run prepublish-test",
    "clean": "rm -rf bin dist lib lib-esm cli-zip",
    "bundle": "webpack --mode=production --optimize-minimize",
    "compile-lib": "npm run compile-es5 && npm run compile-esm && npm run bundle-worker",
    "compile-es5": "rm -rf lib && tsc -p tsconfig.lib.json && node scripts/minify.js lib",
    "compile-esm": "rm -rf lib-esm && tsc -p tsconfig.lib-esm.json && node scripts/minify.js lib-esm",
    "bundle-worker": "webpack --config webpack.config.worker.js --mode=production --optimize-minimize",
    "bundle-cli": "webpack --config webpack.config.server.js --mode=production --optimize-minimize && cp -Rf src/cli/assets bin/cli/assets",
    "publish-cli-s3": "scripts/publish-cli-s3.sh",
    "publish-cdn": "scripts/publish-cdn.sh",
    "pack-sourcemaps": "rm -rf reference-docs.sourcemaps.tag.gz && find lib lib-esm -name \"*.js.map\" | xargs tar -czvf reference-docs.sourcemaps.tag.gz",
    "start": "webpack-dev-server --mode=development --hot",
    "stats": "webpack --json --profile --mode=production -p > stats.json && npx webpack-bundle-analyzer stats.json",
    "prettier": "prettier --write \"src/cli/cli.ts\" \"src/**/*.{ts,tsx}\"",
    "prepublish-test": "scripts/prepublish-test.sh",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:e2e": "cypress run",
    "cypress": "cypress open",
    "list-licenses": "node ./scripts/list-licenses.js",
    "license-check": "license-checker --production --onlyAllow 'MIT;ISC;Apache-2.0;Apache;CC0-1.0;BSD;BSD-2-Clause;BSD-3-Clause;UNKNOWN' --summary",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "precdn-serve": "mkdir -p playground/cdn-test/server-dist && cp -Rf dist/* playground/cdn-test/server-dist && cp -Rf playground/cdn-test/template-source/* playground/cdn-test/server-dist",
    "cdn-serve": "http-server playground/cdn-test/server-dist"
  },
  "peerDependencies": {
    "core-js": "^3.1.4",
    "mobx": "^6.3.2",
    "react": "^16.8.4 || ^17.0.0",
    "react-dom": "^16.8.4 || ^17.0.0",
    "styled-components": "^4.1.1 || ^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/parser": "^7.9.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-syntax-typescript": "^7.8.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/codemirror": "0.0.88",
    "@types/dompurify": "^2.0.2",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^25.1.4",
    "@types/json-pointer": "^1.0.30",
    "@types/lodash": "^4.14.149",
    "@types/mark.js": "^8.11.5",
    "@types/node": "^14.14.37",
    "@types/prismjs": "^1.16.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.4",
    "@types/react-tabs": "^2.3.2",
    "@types/styled-components": "^5.1.9",
    "@types/webpack": "^4.41.9",
    "@types/webpack-env": "^1.14.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babylon": "7.0.0-beta.47",
    "core-js": "^3.5.0",
    "css-loader": "^3.3.2",
    "cypress": "^5.6.0",
    "enzyme": "^3.7.0",
    "enzyme-to-json": "^3.6.2",
    "html-webpack-plugin": "^4.0.3",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.4",
    "json-schema": "^0.2.5",
    "lodash": "^4.17.20",
    "mobx": "^6.3.2",
    "husky": "^4.2.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.0.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^17.0.2",
    "string-replace-loader": "^2.2.0",
    "style-loader": "^1.0.1",
    "terser": "^5.6.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "typescript": "~3.8.3",
    "unfetch": "^4.1.0",
    "url-polyfill": "^1.1.7",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^2.0.0",
    "workerize-loader": "^1.3.0",
    "yaml-js": "^0.2.3"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    },
    "testPathIgnorePatterns": [
      "cypress"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "coveragePathIgnorePatterns": [
      "\\.d\\.ts$",
      "/benchmark/",
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "/benchmark/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/empty.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "arrowParens": "avoid"
  }
}
